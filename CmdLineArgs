import java.io.*;
public class CmdLineArgsDemoV3 extends Object
{
    public static void main(String args[]) 
    {
        CmdLineArgsDemoV3 Obj = new CmdLineArgsDemoV3();
        if(args.length!=2)
        {
            System.out.println("Invalid number of arguments \n you need to pass two positive integers");
        }
        else
        {
        int x = Integer.parseInt(args[0]);
        int y = Integer.parseInt(args[1]);
        int z = x+y;
        System.out.println(" The class name is:- " + Obj.getClass());
        System.out.println(" Sum of " +x+ " end " +y+ " is " +z);
        }
    }
}
